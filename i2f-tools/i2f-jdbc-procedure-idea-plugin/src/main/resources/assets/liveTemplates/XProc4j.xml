        <variable name="BODY" expression="" defaultValue="&quot;&quot;" alwaysStopAt="true"/>
        <context>
            <option name="XML" value="true"/>
        </context>
    </template>
    <template name="&lt;lang-finally" value=" &lt;lang-finally&gt;&#10;    $END$&#10;&lt;/lang-finally&gt;"
