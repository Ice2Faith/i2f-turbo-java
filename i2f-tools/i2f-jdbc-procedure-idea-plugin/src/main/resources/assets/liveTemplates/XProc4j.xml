        <variable name="ITEM" expression="" defaultValue="&quot;item&quot;" alwaysStopAt="true"/>
        <variable name="FIRST" expression="" defaultValue="&quot;first=\&quot;first\&quot;&quot;" alwaysStopAt="true"/>
        <variable name="INDEX" expression="" defaultValue="&quot;index=\&quot;index\&quot;&quot;" alwaysStopAt="true"/>
        <context>
            <option name="XML" value="true"/>
        </context>
    </template>
    <template name="&lt;lang-fori"
