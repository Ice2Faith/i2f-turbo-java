<templateSet group="XProc4j">
    <!-- todo @tag sql-dialect-->
    <template name="&lt;sql-dialect"
              value="&lt;sql-dialect $SCRIPT$ databases=&quot;$DATABASES$&quot;&gt;&#10;    $END$&#10;&lt;/sql-dialect&gt;"
              toReformat="false" toShortenFQNames="true">
        <variable name="SCRIPT" expression="" defaultValue="&quot;script=\&quot;v_sql\&quot;&quot;"
                  alwaysStopAt="true"/>
        <variable name="DATABASES" expression="" defaultValue="&quot;mysql,mariadb&quot;" alwaysStopAt="true"/>
        <context>
            <option name="XML" value="true"/>
        </context>
    </template>
    <template name="&lt;debugger" value="&lt;debugger tag=&quot;$TAG$&quot; test=&quot;$TEST$&quot;/&gt;&#10;$END$"
              toReformat="false" toShortenFQNames="true">
        <variable name="TAG" expression="concat(fileName(),&quot;:&quot;,lineNumber())" defaultValue=""
                  alwaysStopAt="true"/>
        <variable name="TEST" expression="" defaultValue="&quot;true&quot;" alwaysStopAt="true"/>
        <context>
            <option name="XML" value="true"/>
        </context>
    </template>
    <template name="&lt;function-call"
              value="&lt;function-call refid=&quot;$ID$&quot; result=&quot;$RESULT$&quot; $PARAM_SHARE$&#10;    $PARAMS$ $ATTR$/&gt;&#10;$END$"
              toReformat="false" toShortenFQNames="true">
        <variable name="ID" expression="" defaultValue="&quot;&quot;" alwaysStopAt="true"/>
        <variable name="RESULT" expression="" defaultValue="&quot;&quot;" alwaysStopAt="true"/>
        <variable name="PARAM_SHARE" expression="" defaultValue="&quot;params_share=\&quot;true\&quot;&quot;"
                  alwaysStopAt="true"/>
        <variable name="PARAMS" expression="" defaultValue="&quot;params=\&quot;callParams\&quot;&quot;"
                  alwaysStopAt="true"/>
        <variable name="ATTR" expression="" defaultValue="&quot;&quot;" alwaysStopAt="true"/>
        <context>
            <option name="XML" value="true"/>
        </context>
    </template>
    <template name="&lt;java-call"
              value="&lt;java-call result=&quot;$RESULT$&quot; target=&quot;$TARGET$&quot;/&gt;&#10;$END$"
              toReformat="false" toShortenFQNames="true">
        <variable name="RESULT" expression="" defaultValue="&quot;&quot;" alwaysStopAt="true"/>
        <variable name="TARGET" expression="" defaultValue="&quot;&quot;" alwaysStopAt="true"/>
        <context>
            <option name="XML" value="true"/>
        </context>
    </template>
    <template name="&lt;lang-catch"
