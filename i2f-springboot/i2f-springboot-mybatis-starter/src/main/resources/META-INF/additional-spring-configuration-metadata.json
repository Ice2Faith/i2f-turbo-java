{
  "groups": [
    {
      "name": "i2f.mybatis",
      "type": "i2f.springboot.mybatis.MybatisDynamicAutoConfiguration",
      "sourceType": "i2f.springboot.mybatis.MybatisDynamicAutoConfiguration"
    }
  ],
  "properties": [
    {
      "name": "i2f.mybatis.dynamic.enable",
      "type": "java.lang.Boolean",
      "description": "whether enable dynamic script executor.",
      "sourceType": "i2f.springboot.mybatis.dynamic.SpringMybatisDynamicScriptExecutor",
      "defaultValue": true
    },
    {
      "name": "i2f.springboot.config.mybatis.enable",
      "type": "java.lang.Boolean",
      "description": "whether enable mybatis customer configuration.",
      "sourceType": "i2f.springboot.mybatis.MybatisAutoConfiguration",
      "defaultValue": true
    },
    {
      "name": "i2f.springboot.config.mybatis.interceptor.enable",
      "type": "java.lang.Boolean",
      "description": "whether enable mybatis interceptor.",
      "sourceType": "i2f.springboot.mybatis.MybatisAutoConfiguration",
      "defaultValue": true
    },
    {
      "name": "i2f.springboot.config.mybatis.enable-pagination-interceptor",
      "type": "java.lang.Boolean",
      "description": "whether enable mybatis pagination interceptor.",
      "sourceType": "i2f.springboot.mybatis.MybatisAutoConfiguration",
      "defaultValue": false
    }
  ],
  "hints": [
    {
      "name": "server.servlet.jsp.class-name",
      "providers": [
        {
          "name": "class-reference",
          "parameters": {
            "target": "javax.servlet.http.HttpServlet"
          }
        }
      ]
    },
    {
      "name": "server.tomcat.accesslog.encoding",
      "providers": [
        {
          "name": "handle-as",
          "parameters": {
            "target": "java.nio.charset.Charset"
          }
        }
      ]
    }
  ]
}
