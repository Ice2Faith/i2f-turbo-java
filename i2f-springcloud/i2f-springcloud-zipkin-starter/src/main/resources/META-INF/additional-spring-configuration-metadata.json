{
  "groups": [
    {
      "name": "i2f.springcloud.zipkin",
      "type": "i2f.springcloud.zipkin.ZipkinAutoConfiguration",
      "sourceType": "i2f.springcloud.zipkin.ZipkinAutoConfiguration"
    }
  ],
  "properties": [
    {
      "name": "i2f.springcloud.zipkin.enable",
      "type": "java.lang.Boolean",
      "description": "enable zipkin.",
      "sourceType": "i2f.springcloud.zipkin.ZipkinAutoConfiguration",
      "defaultValue": true
    }
  ],
  "hints": [
    {
      "name": "server.servlet.jsp.class-name",
      "providers": [
        {
          "name": "class-reference",
          "parameters": {
            "target": "javax.servlet.http.HttpServlet"
          }
        }
      ]
    },
    {
      "name": "server.tomcat.accesslog.encoding",
      "providers": [
        {
          "name": "handle-as",
          "parameters": {
            "target": "java.nio.charset.Charset"
          }
        }
      ]
    }
  ]
}
